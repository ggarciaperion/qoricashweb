{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://app.qoricash.pe/api/:path*"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://app.qoricash.pe/socket.io/:path*"
    }
  ]
}
